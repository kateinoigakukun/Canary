# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

install! 'cocoapods', :integrate_targets => false
target 'APIModel' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for APIModel

  target 'APIModelTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'Canary' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Canary
  pod 'IBLinter', '0.4.2'
  pod 'SwiftLint'

  target 'CanaryTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CanaryUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'CanaryKit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CanaryKit

  target 'CanaryKitTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'Repository' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Repository

  target 'RepositoryTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
